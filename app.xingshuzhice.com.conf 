server { #listening port
    server_name  app.xingshuzhice.com; #Your domain server_name

    gzip on;
    gzip_min_length 1k;
    gzip_buffers 4 16k;
    gzip_comp_level 2;
    gzip_types text/plain application/javascript application/x-javascript text/css application/xml text/javascript application/x-httpd-php image/jpeg image/gif image/png application/wasm;
    gzip_vary off;
    gzip_disable "MSIE [1-6]\.";

    default_type application/wasm;



    root /data/wwwroot/app.xingshuzhice.com;
    index index.html;

    location / {
        try_files $uri $uri/ /index.html;
    }


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/app.xingshuzhice.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/app.xingshuzhice.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}





server {
    if ($host = app.xingshuzhice.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen       80;
    server_name  app.xingshuzhice.com;
    return 404; # managed by Certbot
}
server {
    if ($host = app.xingshuzhice.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name  app.xingshuzhice.com;
    listen 80;
    return 404; # managed by Certbot


}